<?page title="new page title" contentType="text/html;charset=UTF-8"?>
<zk>
	<style>
		<![CDATA[ .fixed-center .header { padding-left: 8px;
		padding-right: 8px; } .center-left, .center-right { border:
		none; padding-right: 8px; } .center { border: 1px solid #CFCFCF;

		} ]]>
	</style>
	<include id="includeStyle" src="style/default.zul" />
	<hlayout width="100%" height="900px">
