				<div vflex="1" hflex="3" class="center-left">
					<include src="template/sidebar.zul"
						class="center-left-inner" />
				</div>
				<div vflex="1" hflex="7" class="center-right"
					self="@insert(content)">
					<!-- include home.zul or nissan.zul after -->
				</div>
			</hlayout>
			<include vflex="1" class="footer" src="template/footer.zul" />
		</div>
		<div hflex="1"></div>
	</hlayout>
</zk>
